local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Monarch Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "Monarch"})
local catalog
local PurchaseItem
local PurchaseItemListing


local LocalPlayer = Window:MakeTab({
	Name = "Local Player",
	Icon = "rbxassetid://10055529778",
	PremiumOnly = false
})

LocalPlayer:AddButton({
	Name = "Anti Afk",
	Callback = function()
      		
    local vu = game:GetService("VirtualUser")
	game:GetService("Players").LocalPlayer.Idled:connect(function()
		vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
		wait(1)
		vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
	end)
      		
  	end    
})

LocalPlayer:AddToggle({
	Name = "Appear Afk",
	Default = false,
	Callback = function(t)
		      if t then
      
      local args = {
    [1] = true
}

game:GetService("ReplicatedStorage").Remotes.AFK:FireServer(unpack(args))
else
    local args = {
    [1] = false
}

game:GetService("ReplicatedStorage").Remotes.AFK:FireServer(unpack(args))

      
      
      end
  
  
  
	end    
})
local TTT = false
LocalPlayer:AddToggle({
	Name = "Admin Checker",
	Default = false,
	Callback = function(t)
			local Mod = require(game:GetService("ReplicatedStorage").Modules.Admins);
  if t then
 TTT = true
 else
     TTT = false
 
  end


 
 game.Players.PlayerAdded:Connect(function(Player)
     local Player = Player
    local HeadTag = cha.HeadTag
    local Title = HeadTag.Title
    
    if string.find(Title.Text, "mod") or string.find(Title.Text, "owner") or string.find(Title.Text, "admin") then
         local module = loadstring(game:HttpGet"https://raw.githubusercontent.com/LeoKholYt/roblox/main/lk_serverhop.lua")()

            module:Teleport(game.PlaceId)
 end
    
 end)
 
 for i,v in pairs(game.Players:GetPlayers()) do
    
    local cha = v.Character
    local HeadTag = cha.HeadTag
    local Title = HeadTag.Title
    
    if string.find(Title.Text, "mod") or string.find(Title.Text, "owner") or string.find(Title.Text, "admin") then
         local module = loadstring(game:HttpGet"https://raw.githubusercontent.com/LeoKholYt/roblox/main/lk_serverhop.lua")()

            module:Teleport(game.PlaceId)
 end
     end
	end    
})

local LimitedSniper = Window:MakeTab({
	Name = "Limited Sniper",
