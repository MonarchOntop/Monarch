		Item = Value
	end    
})







LimitedSniper:AddToggle({
	Name = "Start limited sniper (specific item only!)",
	Default = false,
	Callback = function(state)
		
			    if state then
	        
	while task.wait(3) and state do
    pcall(function()
    local look = Item
local sex = Purchaseltem
for i,v in pairs(catalog:InvokeServer()) do
local Eprice

local namee = v.name

if string.find(namee, look)  then
    if v.status == "new" then
   print(namee) 
    sex:InvokeServer(namee)
    else
        print("no sex")
end
end
end
end)
end
	        
	    end
		
	end    
})

local Debug = Window:MakeTab({
	Name = "(OPEN!) Debug",
	Icon = "rbxassetid://10055529778",
	PremiumOnly = false
})

Debug:AddParagraph("Paragraph","To prevent you from being banned by firing false remotes, you are REQUIRED to input them. Please use the remote spy provided to find the remotes. First execute it and then refresh the catalog. Copy the name and input it into the 'Catalog' text box. Next, buy an item that is NOT limited yet and repeat what you did previously. Do this for buying a listed/already limited item. ")

Debug:AddButton({
	Name = "Remote spy",
	Callback = function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/CoochieSlayer6372/SimpleSpyFixed/main/badscript2.lua"))()
  	end    
})

Debug:AddTextbox({
	Name = "Catalog remote name",
	Default = "input",
	TextDisappear = true,
	Callback = function(Value)
		catalog = game:GetService("ReplicatedStorage").Remotes:FindFirstChild(Value)
	end	  
})

Debug:AddTextbox({
	Name = "Purchase Item remote name",
	Default = "input",
	TextDisappear = true,
	Callback = function(Value)
		Purchaseltem = game:GetService("ReplicatedStorage").Remotes:FindFirstChild(Value)
	end	  
})

Debug:AddTextbox({
	Name = "Purchase Item Listing remote name",
	Default = "input",
	TextDisappear = true,
	Callback = function(Value)
		PurchaseItemListing = game:GetService("ReplicatedStorage").Remotes:FindFirstChild(Value)
	end	  
})

