local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Monarch Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "Monarch"})
local catalog
local PurchaseItem
local PurchaseItemListing
local RequestItemInfo

local LocalPlayer = Window:MakeTab({
	Name = "Local Player",
	Icon = "rbxassetid://10055529778",
	PremiumOnly = false
})

LocalPlayer:AddButton({
	Name = "Anti Afk",
	Callback = function()
      		
    local vu = game:GetService("VirtualUser")
	game:GetService("Players").LocalPlayer.Idled:connect(function()
		vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
		wait(1)
		vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
	end)
      		
  	end    
})

LocalPlayer:AddToggle({
	Name = "Appear Afk",
	Default = false,
	Callback = function(t)
		      if t then
      
      local args = {
    [1] = true
}

game:GetService("ReplicatedStorage").Remotes.AFK:FireServer(unpack(args))
else
    local args = {
    [1] = false
}

game:GetService("ReplicatedStorage").Remotes.AFK:FireServer(unpack(args))

      
      
      end
  
  
  
	end    
})
local TTT = false
LocalPlayer:AddToggle({
	Name = "Admin Checker",
	Default = false,
	Callback = function(t)
			local Mod = require(game:GetService("ReplicatedStorage").Modules.Admins);
  if t then
 TTT = true
 else
     TTT = false
 
  end


 
 game.Players.PlayerAdded:Connect(function(Player)
     local Player = Player
    local HeadTag = cha.HeadTag
    local Title = HeadTag.Title
    
    if string.find(Title.Text, "mod") or string.find(Title.Text, "owner") or string.find(Title.Text, "admin") then
         local module = loadstring(game:HttpGet"https://raw.githubusercontent.com/LeoKholYt/roblox/main/lk_serverhop.lua")()

            module:Teleport(game.PlaceId)
 end
    
 end)
 
 for i,v in pairs(game.Players:GetPlayers()) do
    
    local cha = v.Character
    local HeadTag = cha.HeadTag
    local Title = HeadTag.Title
    
    if string.find(Title.Text, "mod") or string.find(Title.Text, "owner") or string.find(Title.Text, "admin") then
         local module = loadstring(game:HttpGet"https://raw.githubusercontent.com/LeoKholYt/roblox/main/lk_serverhop.lua")()

            module:Teleport(game.PlaceId)
 end
     end
	end    
})

local LimitedSniper = Window:MakeTab({
	Name = "Limited Sniper",
	Icon = "rbxassetid://10055529778",
	PremiumOnly = false
})
local Item



local ItemDropDown = LimitedSniper:AddDropdown({
	Name = "Select Limited",
	Default = "1",
	Options = {
	    "Dominus",
		"Federation",
        "Sparkle Time Fedora",
        "Domino Crown",
        "Ice Crown",
        "Clockwork",
        "):",
	"Valk"},
	
	Callback = function(Value)
		Item = Value
	end    
})







LimitedSniper:AddToggle({
	Name = "Start limited sniper (specific item only!)",
	Default = false,
	Callback = function(state)
		
			    if state then
	        
	while task.wait(3) and state do

    local look = Item
local sex = Purchaseltem
for i,v in pairs(catalog:InvokeServer()) do
local Eprice

local namee = v.name

if string.find(namee, look)  then
    if v.status == "new" then
   print(namee) 
    sex:InvokeServer(namee)
    else
        print("no sex")
end
end
end

end
	        
	    end
		
	end    
})
local Budget
LimitedSniper:AddSlider({
	Name = "Budget",
	Min = 1,
	Max = 500000,
	Default = 1,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Budget",
	Callback = function(Value)
		Budget = Value
	end    
})
local Discount
LimitedSniper:AddSlider({
	Name = "Discount",
	Min = 1,
	Max = 99,
	Default = 1,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Discount",
	Callback = function(Value)
		Discount = Value
	end    
})

LimitedSniper:AddToggle({
	Name = "Start limited sniper (buys ALL limiteds that are discounted/under your budget)",
	Default = false,
	Callback = function(Value)
		if Value then
		    while task.tick(3) do
		        local sex = PurchaseItemListing
                for i,v in pairs(catalog:InvokeServer()) do
                local Eprice = v.price
                local RAP = v.RAP
                local namee = v.name
                
                local aa = Eprice/RAP*100
                
                if aa <= Discount and Eprice <= budget and v.status ~= "new" then
                    for i,v in pairs(RequestItemInfo:InvokeServer(v.name).listings[1]) do
                    if i == "id" and v.Name <= budget then
                    PurchaseItemListing:InvokeServer(v.Name, v)
                    end
                    end
                    end
            end
		    end
		    end
	end    
})

